<!DOCTYPE configuration>
<service name="Actividad">
	<Description>
		Interfaz para manipular el paquete de Actividad de la Red Social Educativa.
	</Description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
		<messageReceiver  mep="http://www.w3.org/2004/08/wsdl/in-out"  class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</messageReceivers>
	<parameter name="ServiceClass" locked="false">fiuba.taller.actividad.ActividadControlador</parameter>
</service>
